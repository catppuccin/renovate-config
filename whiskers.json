{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "catppuccin/toolbox"
      ],
      "versioning": "regex:^whiskers-v(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)?$"
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^.*.tera$"
      ],
      "matchStrings": [
        "( *)?version: (?<currentValue>.*?)\n"
      ],
      "depNameTemplate": "catppuccin/toolbox",
      "packageNameTemplate": "whiskers",
      "versioningTemplate": "semver-coerced",
      "datasourceTemplate": "github-tags"
    }
  ]
}
