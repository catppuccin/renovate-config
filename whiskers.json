{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^.*.tera$"
      ],
      "matchStrings": [
        "^version: (?<currentValue>.*?)$"
      ],
      "depNameTemplate": "catppuccin/toolbox",
      "versioningTemplate": "semver-coerced",
      "datasourceTemplate": "github-tags"
    }
  ]
}
